programa
{
	
real n1, n2, n3, n4, n5, n6, n7, n8, n9, n10
	real media 
	funcao inicio()
	{
		escreva("Digite um número: ")
		leia(n1)
		escreva("Digite outro número: ")
		leia(n2)
		escreva("Digite outro número: ")
		leia(n3)
		escreva("Digite outro número: ")
		leia(n4)
		escreva("Digite outro número: ")
		leia(n5)
		escreva("Digite outro número: ")
		leia(n6)
		escreva("Digite outro número: ")
		leia(n7)
		escreva("Digite outro número: ")
		leia(n8)
		escreva("Digite outro número: ")
		leia(n9)
		escreva("Digite outro número: ")
		leia(n10)

		media = (n1 + n2 + n3 + n4 + n5 + n6 + n7 + n8 + n9 + n10) / 10
		// Infelizmente não encontrei uma maneira de encurtar essta parte
		escreva("Números informados: ", n1, ", ", n2, ", ", n3, ", ", n4, ", ", n5, ", ", n6, ", ", n7, ", ", n8, ", ", n9, ", ", n10, "\n")
		escreva("Resultado da média: ", media)
	}
}
